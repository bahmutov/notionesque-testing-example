{
  "name": "vite-template-redux",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "tsc --skipLibCheck --noEmit && vite build",
    "preview": "vite preview",
    "test": "cypress run",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "type-check": "tsc --noEmit",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "local": "start-test dev http://localhost:5173/ cy:open",
    "clean": "rm -rf coverage .nyc_output",
    "e2e": "start-test dev http://localhost:5173/ cy:run",
    "pree2e": "npm run clean"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^18.0.1",
    "@reduxjs/toolkit": "^2.0.1",
    "@tailwindcss/vite": "^4.0.14",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "redux-persist": "^6.0.0",
    "redux-undo": "^1.1.0",
    "tailwindcss": "^4.0.14",
    "uuid": "^11.1.0",
    "vite-plugin-istanbul": "^7.0.0"
  },
  "devDependencies": {
    "@bahmutov/cypress-code-coverage": "^2.7.2",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "check-code-coverage": "^1.10.5",
    "cy-spok": "^1.5.2",
    "cypress": "^14.2.0",
    "cypress-cdp": "^1.6.51",
    "cypress-fail-fast": "^7.1.1",
    "cypress-map": "^1.46.0",
    "cypress-on-fix": "^1.1.0",
    "cypress-plugin-steps": "^1.1.1",
    "cypress-real-events": "^1.14.0",
    "cypress-watch-and-reload": "^1.10.18",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.1",
    "start-server-and-test": "^2.0.11",
    "typescript": "^5.8.2",
    "vite": "^6.2.2"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.js",
      "src/**/*.jsx",
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "exclude": [
      "src/types/index.ts",
      "src/**/*.cy.tsx"
    ]
  }
}
